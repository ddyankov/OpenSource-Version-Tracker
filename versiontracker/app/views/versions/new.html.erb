<h1>New version</h1>

<% form_for(@version) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :version_num %><br />
    <%= f.text_field :version_num %>
  </p>
  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', versions_path %>